class Solution(object):
    def lengthOfLongestSubstring(self, s):
        """
        :type s: str
        :rtype: int
        """
        l = 0
        r = 0
        max_len = 0
        wrd = set()
        while r < len(s):

            while l < r and s[r] in wrd:
                wrd.remove(s[l])
                l += 1
            if r < len(s):
                wrd.add(s[r])
                r += 1
            max_len = max(len(wrd), max_len)
            print(max_len, len(wrd), wrd)
        return max_len

